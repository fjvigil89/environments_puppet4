#!/bin/bash

fqdn=$1

ipaddress=`dig +short $fqdn`

case $ipaddress in
  157.193.122.*|\
  157.193.32.*|\
  157.193.33.*|\
  157.193.34.*|\
  157.193.35.*|\
  157.193.40.*|\
  157.193.42.*|\
  157.193.43.*|\
  157.193.44.*|\
  157.193.47.*|\
  157.193.49.*|\
  157.193.71.*|\
  172.16.176.*|\
  172.16.190.*)
    /etc/puppetlabs/code/environments/production/bin/autosign-serverbeheer $fqdn >> /tmp/output
    exit 0
  ;;
  *)
    exit 1
  ;;
esac
