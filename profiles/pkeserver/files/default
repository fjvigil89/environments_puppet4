default vesamenu.c32
prompt 0
timeout 50

# Local Hard Disk pxelinux.cfg default entry
LABEL 1
    MENU LABEL ^1) Boot local hard drive
    MENU AUTOBOOT
    MENU DEFAULT
    LOCALBOOT 0

# Clonezilla pxelinux.cfg default entry
LABEL 2
    MENU LABEL ^2) Clonezilla Live
    KERNEL clonezilla/live/vmlinuz
    APPEND ramdisk_size=32768 initrd=clonezilla/live/initrd.img boot=live union=overlay username=user config components noswap edd=on nomodeset noeject locales=en_US.UTF-8 keyboard-layouts=NONE net.ifnames=0 ocs_live_extra_param="" ocs_live_keymap="NONE" ocs_live_batch="yes" ocs_lang="en_US.UTF-8" vga=788 ip=frommedia nosplash  fetch=tftp://pke.upr.edu.cu/clonezilla/live/filesystem.squashfs

# Proxmox pxelinux.cfg default entry 
LABEL 3
    MENU LABEL ^3) Install Proxmox 6 - HTTP
    KERNEL data/proxmox6/images/pxeboot/vmlinuz
    APPEND initrd=data/proxmox6/images/pxeboot/initrd.img method=http://http://ftp.upr.edu.cu/Software/Por_Sistemas_Operativos/Linux/SO/Proxmox/proxmox-ve_6.1-1.iso devfs=nomount

# Ubuntu 18 pxelinux.cfg default entry 
LABEL 4
    MENU LABEL ^4) Install Ubuntu 18 - HTTP
    KERNEL data/ubuntu18_64Bit/images/pxeboot/vmlinuz
    APPEND initrd=data/ubuntu18_64Bit/images/pxeboot/initrd.img method=http://http://ftp.upr.edu.cu/Software/Por_Sistemas_Operativos/Linux/SO/Ubuntu/Ubuntu/ubuntu-18.04.1-desktop-amd64.iso devfs=nomount

